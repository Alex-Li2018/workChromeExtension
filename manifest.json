{
	"manifest_version": 2,
	"name": "解密插件",
	"version": "1.0",
	"description": "欢聚堂解密插件",
	"author": "alex",
	"icons":
	{
		"16": "img/icon.jpg",
		"48": "img/icon.jpg",
		"128": "img/icon.jpg"
	},
	"browser_action": 
	{
		"default_icon": "img/icon.jpg",
		"default_popup": "popup.html"
	},
	"chrome_url_overrides":
	{
		"newtab": "newtab.html"
	},
	"content_scripts": 
	[
		{
			"matches": ["https://www.baidu.com/*"],
			"js": ["js/jquery-1.8.3.js"]
		}
	],
	"web_accessible_resources": 
	[
		"inject.js"
	]
}